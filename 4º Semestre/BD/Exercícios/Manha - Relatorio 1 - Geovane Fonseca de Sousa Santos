19)
Select NomAut
	from Autor
	order by NomAut desc;

20)
Select NomAut, NomPai
	from Autor join Pais
	on Autor.CodPai = Pais.CodPai;

21)
Select *
	from Autor join Pais
	on Pais.CodPai = Autor.CodPai;

22)
Select *
	from Pais natural join Autor;


23) // Para alterar conteudo dos dados
Update Pais
	set NomPai = 'Brazil'
	where CodPai = 1;

24)
Select *
	from Pais;

25)
Rollback;

26)
Select *
	from Pais;

27) // Para elminiar uma linha
Delete Pais
	where CodPai = 1;

28)
Rollback;
Select *
	from Pais;

