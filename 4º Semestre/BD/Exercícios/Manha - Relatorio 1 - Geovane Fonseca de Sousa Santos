    from Unidade
    order by NomUni;

Create table Curso
    (CodCur number (2) not null,
     NomCur char (25) not null,
     CodUni number (2) not null,
     constraint PK_Curso primary key (CodCur),
     constraint FK_Curso_Unidade foreign key (CodUni) references Unidade);


