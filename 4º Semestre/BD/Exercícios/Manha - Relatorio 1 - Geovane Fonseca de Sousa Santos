	(CodAut number(2) not null,
	 NomAut char(35) not null,
	 CodPai number(2) not null,
	 constraint Pk_Autor primary
		key (CodAut),
	 constraint Fk_Autor_Pais foreign
		key (CodPai) references
	  Pais);
	 // on delete, update restrict;

